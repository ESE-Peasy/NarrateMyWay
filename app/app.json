{
  "expo": {
    "name": "NarrateMyWay",
    "slug": "NarrateMyWay",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nmw-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esepeasy.narratemyway",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.esepeasy.narratemyway",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/nmw-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
